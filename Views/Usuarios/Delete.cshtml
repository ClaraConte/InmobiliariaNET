
@{
    @model Inmobiliaria.Models.Usuario
    ViewData["Title"] = "Delete";
}
    <div>
        <h4>Propietario</h4>
        <hr />
        <div class="col-sm-6 alert alert-danger">
            ¿Seguro qué desea eliminar los datos del siguiente propietario?
        </div>
        <dl class="row">
            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.IdUsuario)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.IdUsuario)
            </dt>
            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.UsuarioNombre)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.UsuarioNombre)
            </dt>
            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.UsuarioApellido)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.UsuarioApellido)
            </dt>
            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.UsuarioDni)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.UsuarioDni)
            </dt>
            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.UsuarioTelefono)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.UsuarioTelefono)
            </dt>
            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.UsuarioEmail)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.UsuarioEmail)
            </dt>
        </dl>

        <form asp-action="Delete">
            <input type="submit"  value="Eliminar" class="btn btn-danger" />
        </form>
    </div>

