
@{
    @model Inmobiliaria.Models.Inmueble
    ViewData["Title"] = "Eliminar";
}
    <div>
        <h4>Inmuebles</h4>
        <hr />
        <div class="col-sm-6 alert alert-danger">
            ¿Seguro qué desea eliminar los datos del siguientes inmueble?
        </div>
        <dl class="row">

            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.IdInmueble)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.IdInmueble)
            </dt>
            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.Usuario)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.Usuario.UsuarioNombre)
            </dt>
            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.InmuebleDomicilio)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.InmuebleDomicilio)
            </dt>
            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.InmuebleLatitud)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.InmuebleLatitud)
            </dt>
            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.InmuebleLongitud)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.InmuebleLongitud)
            </dt>
            <dd class="col-sm-2">
                @Html.DisplayNameFor(model => model.InmueblePrecio)
            </dd>
            <dt class="col-sm-10">
                @Html.DisplayFor(model => model.InmueblePrecio)
            </dt>
        </dl>

        <form asp-action="Delete">
            <input type="submit" value="Eliminar" class="btn btn-danger" />
        </form>
    </div>

